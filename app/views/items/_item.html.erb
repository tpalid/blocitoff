    
    <%= content_tag :tr, id: "item-#{item.id}" do %>
            <td><%= item.body %></td>
        <td class="text-center"><%= item.days_left %></td>
        <td class="text-center"><%= link_to "", item, method: :delete, class: 'glyphicon glyphicon-ok', remote: true %></td>
    </tr>
    <% end %>
    
    